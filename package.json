    "lint:report": "eslint --max-warnings=0 --ext .js src -f json-relative > eslint.json",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ditrit/githubator-plugin.git"
  },
  "keywords": [
    "Leto-modelizer",
    "githubator-plugin",
    "github-action"
